{
  "expo": {
    "name": "myregulars",
    "slug": "myregulars",
    "scheme": "myregulars",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/martini-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/martini2.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.laquintadude.myregulars"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/martini-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.laquintadude.myregulars"
    },
    "web": {
      "favicon": "./assets/martini-icon.png"
    },
    "owner": "laquintadude",
    "extra": {
      "eas": {
        "projectId": "4902382b-3e60-43ce-a9d4-53c031273fea"
      }
    },
    "plugins": [
      "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 32,
            "targetSdkVersion": 32,
            "buildToolsVersion": "32.0.0"
          }
        }
    ]
  }
}
